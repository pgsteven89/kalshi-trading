# Kalshi API Credentials
# Generate at: https://kalshi.com/account/api
KALSHI_API_KEY_ID=your_key_id_here
KALSHI_PRIVATE_KEY_PATH=path/to/private_key.pem

# Environment: "sandbox" or "production"
# Always use sandbox for development!
KALSHI_ENVIRONMENT=sandbox

# Risk Management
MAX_POSITION_SIZE=100
MAX_DAILY_LOSS=500
MAX_EXPOSURE_PER_MARKET=200

# Logging
LOG_LEVEL=INFO
